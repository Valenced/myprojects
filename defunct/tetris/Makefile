# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  template_config = debug
endif
ifeq ($(config),release)
  template_config = release
endif

PROJECTS := template

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

template:
ifneq (,$(template_config))
	@echo "==== Building template ($(template_config)) ===="
	@${MAKE} --no-print-directory -C . -f template.make config=$(template_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f template.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   template"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"